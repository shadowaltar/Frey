<Window x:Class="Algorithms.Apps.Maze.MainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Main"
        Width="173"
        Height="251">
    <Grid>
        <Button Width="40"
                Height="25"
                Margin="10,10,0,0"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="GenerateMaze">
            Gen
        </Button>
        <TextBox x:Name="Width"
                 Height="23"
                 Margin="75,40,10,0"
                 VerticalAlignment="Top"
                 Text="30" />
        <Label Width="60"
               Height="27"
               Margin="10,38,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Width&#10;" />
        <Label Width="60"
               Height="27"
               Margin="10,66,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top"
               Content="Height&#10;" />
        <TextBox x:Name="Height"
                 Height="23"
                 Margin="75,68,10,0"
                 VerticalAlignment="Top"
                 Text="30" />
    </Grid>
</Window>
