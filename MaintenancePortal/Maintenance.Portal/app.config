<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"
             requirePermission="false" />
    <section name="Databases"
             type="Maintenance.Common.ApplicationSettings.DatabaseSection, Maintenance.Common" />
    <section name="Screens"
             type="Maintenance.Portal.ApplicationSettings.ScreenSection, Maintenance.Portal" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.6.6.0" newVersion="2.6.6.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Threading.Tasks" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.6.6.0" newVersion="2.6.6.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="Portal.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date][%-5level][%logger] %m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  <Databases>
    <DefaultDatabaseEnvironments>
      <Default System="IMAP" Environment="DEV" />
      <Default System="FMS" Environment="DEV" />
    </DefaultDatabaseEnvironments>
    <DatabaseEnvironments>
      <Database System="IMAP" Environment="DEV" EncryptedConnection="" />
      <Database System="IMAP" Environment="QA" EncryptedConnection="" />
      <Database System="IMAP" Environment="SIT" EncryptedConnection="" />
      <Database System="FMS" Environment="DEV" ServerName="jptkyfms011vl.jp.fid-intl.com" />
      <Database System="FMS" Environment="QA" ServerName="jptkyfms015vl.jp.fid-intl.com" />
      <Database System="FMS" Environment="PROD" ServerName="fmstnlnx.jp.fid-intl.com" />
    </DatabaseEnvironments>
  </Databases>
  <Screens>
    <Screen Name="Asset Map" TileColor="#C86F26" AssemblyName="Maintenance.AssetMaps" ExecutableName="Maintenance.AssetMaps.exe" />
    <Screen Name="Bloomberg PORT (GRITS)" TileColor="#124562" AssemblyName="Maintenance.Grits" ExecutableName="Maintenance.Grits.exe" />
    <Screen Name="Composite Benchmark" TileColor="#A200FF" AssemblyName="Maintenance.CompositeBenchmarks" ExecutableName="Maintenance.CompositeBenchmarks.exe" />
    <Screen Name="Country Override" TileColor="#F09609" AssemblyName="Maintenance.CountryOverrides" ExecutableName="Maintenance.CountryOverrides.exe" />
    <Screen Name="Portfolio" TileColor="#3373F2" AssemblyName="Maintenance.Portfolios" ExecutableName="Maintenance.Portfolios.exe" />
    <Screen Name="PAG Report" TileColor="#32779C" AssemblyName="Maintenance.PagReport" ExecutableName="Maintenance.PagReport.exe" />
  </Screens>
</configuration>
