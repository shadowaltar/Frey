<UserControl x:Class="Trading.StrategyBuilder.Views.Controls.OrSymbol"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Width="18"
             Height="18">
    <Canvas>
        <Path Fill="White"
              Stroke="Black"
              StrokeThickness="2">
            <Path.Data>
                <GeometryGroup>
                    <LineGeometry StartPoint="4,9" EndPoint="14,9" />
                    <LineGeometry StartPoint="9,4" EndPoint="9,14" />
                    <EllipseGeometry Center="9,9"
                                     RadiusX="8"
                                     RadiusY="8" />
                </GeometryGroup>
            </Path.Data>
        </Path>
    </Canvas>
</UserControl>
